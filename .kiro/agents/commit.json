{
  "name": "commit",
  "description": "Git workflow specialist for commits, staging, and proper commit messages",
  "prompt": null,
  "tools": [
    "read",
    "shell",
    "introspect"
  ],
  "allowedTools": ["read"],
  "resources": [
    "file://.kiro/instructions/commit.md",
    "file://CONTRIBUTING.md",
    "file://AGENTS.md"
  ],
  "toolsSettings": {
    "read": {
      "allowedPaths": ["./**"]
    },
    "shell": {
      "autoAllowReadonly": true,
      "allowedCommands": ["git status", "git diff.*", "git add.*", "git commit.*", "git push.*", "git log.*"]
    }
  }
}
